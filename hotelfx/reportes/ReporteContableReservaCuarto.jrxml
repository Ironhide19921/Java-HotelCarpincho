	<queryString language="SQL">
		<![CDATA[select DATE_FORMAT(Fecha, '%Y-%m-%d') as Fecha,
sum(Ingreso) as Ingreso,
sum(Egreso) as Egreso,
sum(Ganancia) as Ganancia
from(
select 
DATE_FORMAT(r.fechaIngreso, '%Y-%m-%d') as Fecha,
count(*) as Ingreso,
0 as Egreso,
sum(senia) as Ganancia
from hotel.reservacuarto as r
group by DATE_FORMAT(r.fechaIngreso, '%Y-%m-%d')
union
select
DATE_FORMAT(r.fechaOut, '%Y-%m-%d') as Fecha,
0 as Ingreso,
count(*) as Egreso, 
sum(r.montoReservaCuarto) as Ganancia
from hotel.reservacuarto as r
group by DATE_FORMAT(r.fechaOut, '%Y-%m-%d')
) as t
group by Fecha 
  ]]>
