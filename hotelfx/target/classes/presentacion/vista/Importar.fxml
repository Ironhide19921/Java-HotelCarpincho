<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" styleClass="bordePrincipal" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.controlador.ControladorImportar">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="900.0">
         <center>
            <Pane prefHeight="421.0" prefWidth="684.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="tablaClientesImportar" layoutX="1.0" layoutY="-4.0" prefHeight="435.0" prefWidth="800.0">
                    <columns>
                      <TableColumn fx:id="idCliente" prefWidth="66.0" text="idCliente" />
                      <TableColumn fx:id="Nombre" prefWidth="84.0" text="Nombre" />
                        <TableColumn fx:id="Apellido" prefWidth="99.0" text="Apellido" />
                        <TableColumn fx:id="TipoDocumento" prefWidth="71.0" text="TipoDoc " />
                        <TableColumn fx:id="Documento" prefWidth="89.0" text="Documento" />
                        <TableColumn fx:id="Email" prefWidth="116.0" text="Email" />
                        <TableColumn fx:id="Telefono" prefWidth="85.0" text="Telefono" />
                        <TableColumn fx:id="Estado" prefWidth="58.0" text="Estado" />
                        <TableColumn fx:id="FechaNacimiento" prefWidth="130.0" text="FechaNacimiento" />
                    </columns>
                  </TableView>
               </children>
            </Pane>
         </center>
         <top>
            <Pane prefHeight="47.0" prefWidth="900.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnRefrescar" layoutX="699.0" layoutY="5.0" mnemonicParsing="false" onAction="#refrescarTabla" prefHeight="30.0" prefWidth="150.0" text="Refrescar" />
               </children>
            </Pane>
         </top>
         <left>
            <Pane prefHeight="0.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Pane prefHeight="200.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <Pane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnImportar" layoutX="220.0" layoutY="15.0" mnemonicParsing="false" onAction="#importarCSV" prefHeight="30.0" prefWidth="150.0" text="Importar" />
                  <Button fx:id="btnInsertar" layoutX="52.0" layoutY="15.0" mnemonicParsing="false" onAction="#insertarClientesNuevos" prefHeight="30.0" prefWidth="150.0" text="Insertar" />
                  <Button fx:id="btnMail" layoutX="701.0" layoutY="15.0" mnemonicParsing="false" onAction="#manejoMail" prefHeight="30.0" prefWidth="150.0" text="Mail" />
               </children>
            </Pane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
